### application php项目目录
APPLICATION=./app

### 宿主机端口列表，如果有冲突则可适当修改
MYSQL_PORT=3306
NGINX_PORT=8080
REDIS_PORT=6379
PHPMYADMIN_PORT=81

### mariadb 数据库
MYSQL_ROOT_PASSWORD=root
MYSQL_USER=user
MYSQL_PASSWORD=123456
MYSQL_DATABASE=db
MYSQL_DATA_PATH=./conf/mysql/data
MYSQL_CONFIG_FILENAME=./conf/mysql/conf.d/mysql.cnf


### nginx 
NGINX_CONFIG_FILENAME=./conf/nginx/nginx.conf
NGINX_VHOST_PATH=./conf/nginx/conf.d

### redis
REDIS_DATA_PATH=./conf/redis/data
REDIS_CONFIG_FILENAME=./conf/redis/redis.conf

### php
PHP_INI_PATH=./conf/php/php.ini
PHP_FPM_PATH=./conf/php/php-fpm.conf

### log
LOG_PATH=./logs
